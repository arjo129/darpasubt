<?xml version="1.0"?>
<launch>
  <!-- Arguments -->
  <arg name="robot_name" default="X1_1" />

  <!-- Spawn your team here. -->
  <include file="$(find x1_gazebo)/launch/spawn_x1.launch">
    <arg name="robot_namespace" value="$(arg robot_name)"/>
    <arg name="x" value="1"/>
    <arg name="y" value="1"/>
  </include>

  <!-- Launch your controller nodes -->
  <node name="$(arg robot_name)_control" pkg="subt_seed" type="subt_seed_node" args="$(arg robot_name)" />


</launch>
