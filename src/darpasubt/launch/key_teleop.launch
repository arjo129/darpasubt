<launch>
	<node pkg="key_teleop" type="key_teleop.py" name="key_teleop" output="screen">
    <param name="hz" value="10"/>
    <param name="forward_rate" value="2.0"/>
    <param name="backward_rate" value="2.0"/>
    <param name="rotation_rate" value="1.0"/>
    <remap from="/key_vel" to="/X1_1/cmd_vel"/>
  </node>
</launch>
